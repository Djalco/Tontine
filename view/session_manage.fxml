<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox spacing="25.0" stylesheets="@../css/light.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.SessionManageController">
   <children>
      <VBox>
         <children>
            <HBox styleClass="header">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Recherche" />
                  <Pane HBox.hgrow="ALWAYS" />
                  <Button fx:id="searchBtn" mnemonicParsing="false">
                     <graphic>
                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../img/search.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
               </children>
            </HBox>
            <Separator VBox.vgrow="ALWAYS" />
            <HBox spacing="10.0" styleClass="body" VBox.vgrow="ALWAYS">
               <children>
                  <HBox>
                     <children>
                        <VBox spacing="15.0">
                           <children>
                              <VBox>
                                 <children>
                                    <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Num Cotisation" />
                                    <ComboBox fx:id="searchNum" prefWidth="150.0" />
                                 </children>
                              </VBox>
                              <VBox>
                                 <children>
                                    <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Statut Cotisation" />
                                    <ComboBox fx:id="searchContribute" prefWidth="150.0" />
                                 </children>
                              </VBox>
                           </children>
                        </VBox>
                        <Pane maxWidth="250.0" minWidth="25.0" HBox.hgrow="ALWAYS" />
                        <VBox spacing="15.0">
                           <children>
                              <VBox>
                                 <children>
                                    <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Statut Pret" />
                                    <ComboBox fx:id="searchLoan" prefWidth="150.0" />
                                 </children>
                              </VBox>
                              <VBox>
                                 <children>
                                    <Text strokeType="OUTSIDE" strokeWidth="0.0" text="NOMBRE DE RESULTAT" />
                                    <ComboBox fx:id="searchNumResult" prefWidth="150.0" />
                                 </children>
                              </VBox>
                           </children>
                        </VBox>
                        <Pane maxWidth="250.0" minWidth="25.0" HBox.hgrow="ALWAYS" />
                        <VBox spacing="15.0">
                           <children>
                              <VBox>
                                 <children>
                                    <Text strokeType="OUTSIDE" strokeWidth="0.0" text="ORDRE" />
                                    <ComboBox fx:id="searchOrder" prefWidth="150.0" />
                                 </children>
                              </VBox>
                           </children>
                        </VBox>
                     </children>
                  </HBox>
               </children>
            </HBox>
         </children>
         <styleClass>
            <String fx:value="searchBar" />
            <String fx:value="drop-shadow" />
         </styleClass>
      </VBox>
      <VBox minHeight="300.0" spacing="10.0" VBox.vgrow="ALWAYS">
         <children>
            <HBox alignment="CENTER">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" styleClass="title-form" text="Sessions (" />
                  <Text fx:id="titleTable" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="title-form" text="50" />
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" styleClass="title-form" text=")" />
                  <Pane HBox.hgrow="ALWAYS" />
                  <Button fx:id="btnAdd" mnemonicParsing="false">
                     <graphic>
                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../img/add.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
               </children>
            </HBox>
            <TableView fx:id="table" VBox.vgrow="ALWAYS">
               <columns>
                  <TableColumn fx:id="columnNum" prefWidth="75.0" text="Num Session" />
                  <TableColumn fx:id="columnCotisation" prefWidth="75.0" text="Nbres Cotisations" />
                  <TableColumn fx:id="columnLoan" prefWidth="75.0" text="Nbres de Prets" />
                  <TableColumn prefWidth="75.0" text="Statut">
                     <columns>
                        <TableColumn fx:id="columnStatisContribute" prefWidth="75.0" text="Cotisation" />
                        <TableColumn fx:id="columnStatusLoan" prefWidth="75.0" text="PrÃªt" />
                     </columns></TableColumn>
               </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
            <HBox fx:id="pagination" alignment="CENTER" spacing="10.0" />
         </children>
         <styleClass>
            <String fx:value="tableContainer" />
            <String fx:value="drop-shadow" />
         </styleClass>
      </VBox>
   </children>
</VBox>
